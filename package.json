{
  "name": "react-boilerplate",
  "version": "1.0.0",
  "description": "Basic React Boilerplate",
  "main": "index.js",
  "scripts": {
    "build": "npm run build-server && npm run build-client",
    "build-server": "babel src/server --out-dir ./dist",
    "build-client": "babel src/client --copy-files --out-dir ./dist/public",
    "start": "node ./dist/index.js",
    "test": "jest ./src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/spartanhero93/react-boilerplate.git"
  },
  "keywords": [
    "Node",
    "React"
  ],
  "author": "Luis Lopez",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/spartanhero93/react-boilerplate/issues"
  },
  "homepage": "https://github.com/spartanhero93/react-boilerplate#readme",
  "dependencies": {
    "express": "^4.16.3",
    "react": "^16.3.1",
    "react-dom": "^16.3.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "jest": "^22.4.3"
  }
}
